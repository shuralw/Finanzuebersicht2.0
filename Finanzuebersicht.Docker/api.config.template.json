{
  "AllowedHosts": "*",
  "CorsOrigins": "***",
  "ConnectionStrings": {
    "Finanzuebersicht.Database": "***;TrustServerCertificate=True;MultipleActiveResultSets=True"
  },
  "ContractArchitecture": {
    "Sessions": {
      "Expiration": {
        "RunOnInitialization": true,
        "ExpirationTimeInMinutes": 240
      },
      "Login": {
        "EmailUser": {
          "FailedLoginAttempts": {
            "MaxCount": "5",
            "RunOnInitialization": true,
            "ThresholdInMinutes": 60
          }
        }
      }
    },
    "Users": {
      "EmailUser": {
        "PasswordReset": {
          "ExpirationTimeInMinutes": 60,
          "MailHomepageUrl": "***",
          "MailResetPasswordUrlPrefix": "***/login/reset-password/",
          "MailSupportUrl": "support@example.org",
          "RunOnInitialization": true
        }
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Services": {
    "Smtp": {
      "SmtpSender": "noreply@example.org",
      "SmtpHost": "***",
      "SmtpPort": 25,
      "EnableSsl": false
    },
    "SsoAuthentication": {
      "SsoWebserviceBaseUrl": "***"
    }
  }
}